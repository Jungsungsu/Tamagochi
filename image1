	public void adv4() { // 모험 [1] 실패시 낭떠러지
		System.out.println("                                   \r\n"
				+ "               !-                       \r\n" + "               ,#.                      \r\n"
				+ "                :;            ,*        \r\n" + "                .*            !@        \r\n"
				+ "                ,@:    .!@;   ~=        \r\n" + "                .#=,   *@@@.  ;,        \r\n"
				+ "                 :@#$; #@@$.  *.        \r\n" + "      .=         .=@@@$@@@~  ~@.        \r\n"
				+ "       #-    .    .#@@@@@#;- !;         \r\n" + "       ##!*!*=:    #@@@@@@@#*@~         \r\n"
				+ "       #@@@@@@@$   #@@@@@@@@@*.         \r\n" + "       :~,=##@@@# ;@@@@@@=;,            \r\n"
				+ "             :#@@!=@@@@@!               \r\n" + "              -#@@@@@@@@                \r\n"
				+ "            -=#@@@@@@@@,                \r\n" + "           #@@@@@@@@@@#.                \r\n"
