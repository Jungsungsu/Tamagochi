	public void adv3() { // 모험 [3] 드래곤 레어를 털어보자
		System.out.println("                      \r\n" + "                       :!-        ;;-   \r\n"
				+ "                     ;  @@;       !;##  \r\n" + "                     @* .$@; ,  ::==;$~ \r\n"
				+ "                     $@=*$@#=#: ~-. =#, \r\n" + "                      @@@@@@@@@,   ~@#  \r\n"
				+ "                    *:;@@@@@@-*=:!@@@*  \r\n" + "                    *@#@@@@@@:=@@@@@@#  \r\n"
				+ "            .,;,,    $@@@@@@@@@@@@$$#   \r\n" + "     .     ,#@@@@=    =@@@@@@@@@@; .,   \r\n"
				+ "   ~@@##-  #@@@@@@@ *@@@@@@@@@@*        \r\n" + "   :#@@@@~ @@# *@@@, =@@@@@@@@@@-,,,.   \r\n"
				+ "  *@@@@@@: @@; !@@@.  @@@@@#$@@@@###$   \r\n" + " ,!~,@;$$  @@- $@@@   @@@@*   @@@@@@@   \r\n"
				+ "    - .#$  @@,-@@@,   @@@@    !  =@@:   \r\n" + "      -@$ -@@ ;@@$    =@@@    ! .#:@~   \r\n"
				+ "       $@:@@@ @@@:    ;@@@    !.-; !    \r\n" + "       *@@@@* @@@:    !@@@    !,  .     \r\n"
				+ "        #@@@: @@@@    @@@@    !         \r\n" + "           ~: #@@@@*=@@@@@    !         \r\n"
				+ "           ~: ;@@@@@@@@@@*    !         \r\n" + "           ~:  *@@@@@@@@#-    !         \r\n"
				+ "       *@; ~:   ,!=@@@$!,*@:  !         \r\n" + "      ~@@@ ~:      ,,,. ,@@$.:#.        \r\n"
				+ "       *#; ~:           .=#!.@*         \r\n" + "      ,!*  ~:            ~=!$@-         \r\n"
				+ "    ,@@@@: ~:           #@@@@~          \r\n" + "   -@#@@@! ~:         ~#@@@,            \r\n"
				+ "   !@-@@@@ ~:         ##@@@             \r\n" + "   @; @@@@@$;         @-@@@             \r\n"
				+ "   !,~@@;~!@$        .@,@@@             \r\n" + "     @@@   ,.         ;.@@@             \r\n"
				+ "     @#@#,              @@@*            \r\n" + "     @!~@#              @*@#,           \r\n"
