	}
	public void grade1() {//첫번째 진화
		System.out.println("         ,,!        \r\n" + "         .. ;       \r\n" + "         -@$        \r\n"
				+ "         -@*        \r\n" + "         -@*        \r\n" + "         ,@*        \r\n"
				+ "     ~*@@@@@@~      \r\n" + "    ,--=@@@@@@@,,   \r\n" + "   ;@#:.:@@@@:.-    \r\n"
				+ "  ~@@=~.~@@@#; :$   \r\n" + "  $@#=*;*#@-  .!#-  \r\n" + "  @@@$==$@#==$#@;:  \r\n"
				+ " *@@@@@@@#.*==$@@@  \r\n" + " $@@@@@@@@,    @@@  \r\n" + " $@@@@@@@@@   !@@@  \r\n"
				+ " *@@@@@@@@@#*$@@@@  \r\n" + "  @@@@@@@@@@@@@@@#  \r\n" + "  *@@@@@$=====$@@~  \r\n"
				+ "  -@@@@========$@-  \r\n" + "    @#==========-   \r\n" + "    ,$=========~    \r\n"
				+ "      -$*****;");
		
	}
	public void grade2() {//두번째 진화
		System.out.println("   !* ");
		System.out.println("   #@ ");
		System.out.println("   ..                         ~##  ");
		System.out.println("              -;$@@@@#!:.   .=@@*");
		System.out.println("             $@@=-..-!@@@;  $@-@: ");
		System.out.println("    -$~    ~#$~        ,;@$!#.:@.");
		System.out.println("    ~@;   :@* ,-         .#@! !# ");
		System.out.println("     ,   ,@;  #@.         ~@- $:");
		System.out.println("         ~@-  ;;          -@- #~");
		System.out.println("          =@,             =@; *= ");
		System.out.println("           *@;          ,=#$$ ;@ ");
		System.out.println("            ;@@;      -#@#. @=-@-");
		System.out.println("             .:#@@$#@@@*~   -@$@; ");
		System.out.println("                 -::-        .=##  ");
		System.out.println("                               .,     ");
	}
	public void grade3() {//세번째 진화
		System.out.println("                      ~,      ");
		System.out.println("                     - -");
		System.out.println("                    ,  ;      ");
		System.out.println("                   :  ,;");
		System.out.println("                  :  .-;");
		System.out.println("                 !,--~;;");
		System.out.println("            !!!!    ;;;!");
		System.out.println("         .;.            ..; ");
		System.out.println("         .;.            ..; ");
		System.out.println("     ~.      ,~~          .-;");
		System.out.println("    -        ---;-        .,,,");
		System.out.println(" !. .   @#  ! ~ ;!         ,--");
		System.out.println("..!!    $=  ! ,.~;        ,,~-");
		System.out.println(".,          ! ,-;;       ,--;,");
		System.out.println(" :            --;! .. . .-,,-:");
		System.out.println(" , ..    ... .!;      ..,,-,-;");
		System.out.println(" -   .         ..   ...-,,,-~~");
		System.out.println(" ;,.      ...........,,----,~,");
		System.out.println(" ;;!.           ,.----,,,--.--");
		System.out.println(" :!~-.          ...........,:,");
		System.out.println("    !......................~!");
		System.out.println("     ;~.................~~~~:");
		System.out.println("       ~~~.........~~~~~~~~~~");
		System.out.println("        ..!:~:~~~~~~~~~~~~;.");
		System.out.println("           ,-~;;;;;;;;;;;, ");
		System.out.println("                  ~~~~;;; ");
		System.out.println("                   :. ~;;");
		System.out.println("                    ; .;:");
		System.out.println("                     -.;");
		System.out.println("                      ~!");
	}
