	public void adv5() { //모험 [2] 실패시 벌레한테 물림
		System.out.println("                         ,-!!!!~-    \r\n"
				+ "                    ,~,             ,*.      .*-  \r\n"
				+ "                  .:.  ,~,         ~-          ~~ \r\n"
				+ "           ~:     ,       :       ;             ,,\r\n"
				+ "     ,.   :  .    .       ..    .~.             .,\r\n"
				+ "     ,.  . ,.     .        -   .:               .,\r\n"
				+ "    .   .         -.       ,   -                ;.\r\n"
				+ "    ,  -          ..       .  ~,               ~- \r\n"
				+ "                   !         -                :-  \r\n"
				+ "      ;,:.         ..     .  ~              .:.   \r\n"
				+ "    ,:   ,,.,,.     -     . ~             .~~     \r\n"
				+ "  - :     : ..-*.   -     .,.            --.      \r\n"
				+ " . ,       ,    ~:.  -. . .,  .        ~~         \r\n"
				+ ",  ;  **   :      *   . - ,  ,   .   ~-           \r\n"
				+ ".:=- -$;.  -      .!  ,~!:, ,. ., ,~-             \r\n"
				+ " =$! ~$$; .        .::  !!..,,...,****-.          \r\n"
				+ ",!$$ .$$- ,         :   !*,:..    ===**~-         \r\n"
				+ "  ~ , ,- .          :   !===*,    =====; .,       \r\n"
				+ "  ,.  ;:.           :   !====-    =====!   .      \r\n"
				+ "         ,.         :   !====-    =====!    -     \r\n"
				+ "        ,~.         ~   !====-    =====!    ;.    \r\n"
				+ " ,  ~..;-          .~   !====,    =====:    *:    \r\n"
				+ "  .               .$    *====.    ==$==-    *!,,. \r\n"
				+ "   .              ==    =$$$=     $=$$=.    =$$,  \r\n"
				+ "    ,.          :==-   .$=$$=    .==$$=     =:    \r\n"
				+ "      :... ...~$$$$    -=$$=:    :$$$$$     =.    \r\n"
				+ "        ,,..,, *$$!-   *$$=$     !$$$=*    -,     \r\n"
				+ "             ..-*$;,. .$$$==     *$$$$:   ..      \r\n"
				+ "                 *,   :$$$$;    ,$$$$$~  ,.       \r\n"
