public class image1 {
	public void meet() { //식사 [1] 고기
		System.out.println("           ,,,,,.   \r\n" + "          ,$@@@@*.  \r\n" + "         ,$@@@@@@*. \r\n"
				+ "         :@@@@@@@@, \r\n" + "         !@@@@@@@@, \r\n" + "        -@@@@@@@@@, \r\n"
				+ "        ~@@@@@@@@@, \r\n" + "       .#@@@@@@@@$, \r\n" + "       ;@@@@@@@@$,  \r\n"
				+ "      ;@@@@@@@::,   \r\n" + "      @@@@@;--      \r\n" + " ,####.~@@;         \r\n"
				+ " ~! !, ,#-          \r\n" + " :*.. ,#            \r\n" + " .*==.!#            \r\n"
				+ "  .*~  #            \r\n" + "   ,!;*#            \r\n" + "    .--,            \r\n"
				+ "                    ");
	}
